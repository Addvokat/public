first = int(input())
second = int(input())
third = int(input())


if first >= second and first >= third:
    print(first)
elif second >= first and second >= third:
    print(second)
elif third >= second and third >= first:
    print(third)

if first <= second and first <= third:
    print(first)
elif second <= first and second <= third:
    print(second)
elif third <= second and third <= first:
    print(third)

if first >= third and first <= second or first <= third and first >= second:
    print(first)
elif second >= first and second <= third or second <= first and second >= third:
    print(second)
elif third >= first and third <= second or third <= first and third >= second:
    print(third)
